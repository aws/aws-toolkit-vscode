            },
            "aws-codewhisperer-learn": {
                "description": "AWS Contributed Icon",
                "default": {
                    "fontPath": "./resources/fonts/aws-toolkit-icons.woff",
                    "fontCharacter": "\\f1b7"
                }
            },
            "aws-generic-attach-file": {
